{
  "name": "retype-cli",
  "version": "1.2.1",
  "description": "The TypeScript Refactoring CLI",
  "author": "mvrcoag",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "typescript",
    "refactoring",
    "cli",
    "rename",
    "ts-morph",
    "ast",
    "code-analysis",
    "developer-tools"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvrcoag/retype-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mvrcoag/retype-cli/issues"
  },
  "homepage": "https://github.com/mvrcoag/retype-cli#readme",
  "bin": {
    "retype": "./dist/cli/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "7.2.0",
    "boxen": "8.0.1",
    "chalk": "5.3.0",
    "cli-table3": "0.6.5",
    "commander": "12.1.0",
    "ora": "8.1.0",
    "ts-morph": "24.0.0"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
